package main

import (
	"fmt"
	//"net"
	//"../types"
	//"../com"
	"../order"
	"../driver"
	"../elevator"
	//"time"
)



func main() {

    //initializeriengn
    driver.ElevInit()
    order.Init()
    elevator.Init()
    driver.ElevInitLights()
    elevator.LightsInit()
    for (driver.ElevGetFloorSensorSignal() == -1){
        driver.ElevSetSpeed(-200)
    }
    driver.ElevSetSpeed(0)
    
    //Selveste statemaskinen
    
    for {
        elevator.ControlStateMachine()
        order.UpdateLocalTable(order.LocalOrders)
    }
    
	
	
	
	fmt.Println("End")
}
